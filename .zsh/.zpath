#!/bin/zsh
# Make path's entries unique
typeset -U path

#if [[ -d /opt/local/bin ]]; then
#	path=(/opt/local/bin $path)
#fi
#if [[ -d /opt/local/sbin ]]; then
#	path=(/opt/local/sbin $path)
#fi
#if [[ -d ~/.gem/ruby/1.8/bin ]]; then
#	path=(~/.gem/ruby/1.8/bin $path)
#fi
if [[ -d ~/bin ]]; then
	path=(~/bin $path)
fi
if [[ -d ~/.homebrew/bin  ]]; then
	path=(~/.homebrew/bin $path)

	RUBY_PATH=$(brew --prefix ruby)/bin
	if [[ -d $RUBY_PATH ]]; then
		path=($RUBY_PATH $path)
	fi
fi
