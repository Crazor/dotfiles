#!/bin/zsh
# Make path's entries unique
typeset -U path

#Stick /usr/local/bin to the front
if [[ -d /usr/local/bin ]]; then
	path=(/usr/local/bin $path)
	fi

if [[ -d ~/bin ]]; then
	path=(~/bin /usr/local/sbin $path)
fi

if [[ -d /usr/local/share/npm/bin ]]; then
	path=($path /usr/local/share/npm/bin)
fi
